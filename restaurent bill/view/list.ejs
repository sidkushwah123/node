<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link  rel="stylesheet" href="list.css">
</head>
<body>
    <div class="img-cont">
        <img  style="width: 200px;" src="freso logo.png">
    </div>
    <div class="container-d">
        <h1>users</h1>
    </div>
       <div class="table-design">
        <table class="content-table" >
            <tr>
                <th>firstname</th>
                <th>lastname</th>
                <th>email</th>
                <th>password</th>
            </tr>
            <% data.forEach(function(x)
            {
                %>
            <tr>
                <td><%=x.firstname %></td>
                <td><%=x.lastname %></td>
                <td><%=x.email %></td>
                <td><%=x.password %></td>
                <td> <a href="/edit?id=<%=x.id %>">Edit</a></td>
                <td> <a href="/del?id=<%=x.id %>">Delete</a></td>

            </tr>
            <%  
         })%>
        </table>
    </div>

</body>
</html>